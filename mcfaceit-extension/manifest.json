{
  "name": "Mcface It",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Mcface your browsing experience.",
  "homepage_url": "https://github.com/krawaller/mcfaceit",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions": [
    "https://*/*",
    "http://*/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://*/*",
        "http://*/*"
      ],
      "js": [
        "src/inject/pos.js",
        "src/inject/inject.js"
      ],
      "css" : ["src/inject/boaty.css"]
    }
  ]
}
